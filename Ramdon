#include <iostream>
#include <cstdlib>
#include <ctime>

int main() {
    // Inicializa la semilla del generador de números aleatorios con la hora actual
    std::srand(std::time(0));

    // Genera un número aleatorio entre 0 y 99
    int numeroAleatorio = std::rand() % 100;

    std::cout << "Número aleatorio generado: " << numeroAleatorio << std::endl;

    return 0;
}
